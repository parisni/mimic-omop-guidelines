@article{mimic-omop-previous,
	title = {Conversion of MIMIC to OHDSI CDM},
	shorttitle = {Conversion of MIMIC to OHDSI CDM},
	abstract = {The Observational Medical Outcome Partnership (OMOP) provides a Common Data Model (CDM) for standardizing the format and content for electronic health records (EHRs) and claims data so that that it can be analyzed by a library of standard methods written for OMOP CDM. MIMIC II and MIMIC III are very popular critical care databases that are freely available and quite comprehensive in terms of EHR recordings in ICU settings. However they do not conform to OMOP CDM. We have designed and developed a PostgreSQL-based extraction, transformation and loading (ETL) that generates CDM v5-compatible CSV files from MIMIC II data files. Availability of CDM-shaped MIMIC dataset allows OHDSI researchers to easily work with MIMIC, and also exposes the large and expanding research community working with MIMIC data to leverage and further develop OHDSI tools.},
	language = {en},
	author = {Md Shamsuzzoha Bayzid, Vojtech Huser, Joydeep Ghosh},
	year = {2016},
}
